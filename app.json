{
  "expo": {
    "name": "Glucoller",
    "slug": "glucoller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/onlylogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/glucoller.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/onlylogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anruzulo.App_Glucemia"
    },
    "web": {
      "favicon": "./assets/onlylogo.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7a6d8084-3127-4b7e-a662-b56c1664c8fd"
      }
    }
  }
}